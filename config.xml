<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "ac.id.stmik.moviedb" 
    versionCode = "1"
    version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>Movie DB</name>

    <description>
        Aplikasi untuk mengetahui film terbaru, film yang akan datang serta ratingnya.
    </description>

    <author href="https://stmik-wp.ac.id" email="syahukasyah@gmail.com">
        Ukasyah
    </author>
    <plugin name="cordova-plugin-inappbrowser" />
    <plugin name="cordova-plugin-whitelist" version="1" />

    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />

    <icon src="icon.jpg" />
</widget>